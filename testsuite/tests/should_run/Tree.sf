data BTree [A,B] = Leaf A
                 | Node BTree[A,B] B BTree[A,B]
                 ;

let rec countLeaves[A,B] (tree: BTree[A,B]): Int =
    case tree of
        Leaf a -> 1
      | Node l b r -> countLeaves[A,B] l + countLeaves[A,B] r
      ;
