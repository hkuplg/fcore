include M5
include M4

let null [A] (xs: M5.PolyList[A]): Bool =
    case xs of
       Nil -> True
     | Cons y ys -> False;

let headIs0 (xs : M5.IntList) : Bool =
   case xs of
       Nil -> if null [Int] xs
              then False
              else True -- unreachable
     | Cons y ys -> if null [Int] xs
                    then True -- unreachable
                    else y == 0;

headIs0 (Cons [Int] 0 (Nil [Int]))
